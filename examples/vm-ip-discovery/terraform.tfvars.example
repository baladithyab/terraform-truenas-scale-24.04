# TrueNAS Configuration
# Copy this file to terraform.tfvars and fill in your values

truenas_base_url = "http://YOUR_TRUENAS_IP:PORT"  # e.g., "http://10.0.0.83:81"
truenas_api_key  = "YOUR_API_KEY_HERE"

# SSH Configuration for Guest Agent Queries
truenas_host   = "YOUR_TRUENAS_IP"  # e.g., "10.0.0.83"
ssh_user       = "root"
ssh_key_path   = "~/.ssh/id_rsa"

